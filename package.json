    "next": "^13.3.0",
    "zod": "^3.21.4"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/qs": "^6.9.7",
    "chokidar": "^3.5.3",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.1",
    "qs": "^6.11.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
