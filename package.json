{
  "version": "0.0.41",
  "name": "next-zod-router",
  "packageManager": "yarn@3.4.1",
  "bin": "./tools/index.js",
  "scripts": {
    "test": "vitest run",
    "build": "rimraf ./lib && yarn gen:apitype && tsc",
    "build:tool": "tsc -p ./tools/tsconfig.json",
    "gen:apitype": "ts-node ./tools/src/command.ts --pagesDir=example/pages --baseDir=example"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    },
    "./swr": {
      "import": "./lib/swr.js",
      "require": "./lib/swr.js",
      "types": "./lib/swr.d.ts",
      "default": "./lib/swr.js"
    },
    "./react-query": {
      "import": "./lib/react-query.js",
      "require": "./lib/react-query.js",
      "types": "./lib/react-query.d.ts",
      "default": "./lib/react-query.js"
    }
  },
  "typesVersions": {
    "*": {
      "swr": [
        "./lib/swr.d.ts"
      ],
      "react-query": [
        "./lib/react-query.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "cors": "^2.8.5",
    "next": "^13.3.0",
    "react-query": "^3.39.3",
    "swr": "^2.1.3",
    "vitest": "^0.30.1",
    "zod": "^3.21.4"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/qs": "^6.9.7",
    "chokidar": "^3.5.3",
    "commander": "^10.0.0",
    "fs-extra": "^11.1.1",
    "node-mocks-http": "^1.12.2",
    "qs": "^6.11.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
